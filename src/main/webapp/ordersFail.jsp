<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Order Failed</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css">
    <style>
        body {
            background: linear-gradient(to bottom right, #f7d9d9, #ffffff);
            font-family: 'Poppins', sans-serif;
            overflow-x: hidden;
        }
        .container {
            position: relative;
            z-index: 10;
        }
        .fail-card {
            background: white;
            border-radius: 30px;
            box-shadow: 0 8px 30px rgba(0, 0, 0, 0.1);
            overflow: hidden;
            animation: fadeIn 1.5s ease;
            max-width: 600px;
            margin: 0 auto;
        }
        @keyframes fadeIn {
            from {
                opacity: 0;
                transform: scale(0.9);
            }
            to {
                opacity: 1;
                transform: scale(1);
            }
        }
        .fail-icon {
            font-size: 120px;
            color: #dc3545;
            animation: shake 1.5s infinite;
        }
        @keyframes shake {
            0%, 100% { transform: translateX(0); }
            25% { transform: translateX(-5px); }
            50% { transform: translateX(5px); }
            75% { transform: translateX(-5px); }
        }
        .btn-danger-custom {
            background: linear-gradient(45deg, #ff6b6b, #ff0000);
            color: white;
            border: none;
            font-size: 18px;
            padding: 10px 30px;
            border-radius: 50px;
            transition: all 0.4s ease;
        }
        .btn-danger-custom:hover {
            background: linear-gradient(45deg, #ff0000, #ff6b6b);
            transform: translateY(-3px);
        }
        .floating-icons {
            position: absolute;
            animation: float 6s infinite ease-in-out;
        }
        .icon-1 {
            top: 10%;
            left: 5%;
        }
        .icon-2 {
            bottom: 20%;
            right: 5%;
            animation-delay: 1.5s;
        }
        @keyframes float {
            0%, 100% { transform: translateY(0); }
            50% { transform: translateY(-15px); }
        }
    </style>
</head>
<body>
    <!-- Floating Icons for Visual Appeal -->
    <img src="data:image/png;base64," alt="Error Icon 1" class="floating-icons icon-1" style="width: 100px;">
    <img src="data:image/jpeg;base64," alt="Error Icon 2" class="floating-icons icon-2" style="width: 120px;">

    <div class="container text-center d-flex align-items-center justify-content-center vh-100">
        <div class="fail-card p-5 text-center">
            <div>
                <i class="fail-icon bi bi-x-circle-fill"></i>
            </div>
            <h1 class="display-4 text-danger fw-bold">Order Failed</h1>
            <p class="text-muted fs-5 mt-3">Something went wrong while placing your order. Please try again later.</p>
            <div class="mt-4">
                <a href="home.jsp" class="btn btn-danger-custom">Back to Home</a>
            </div>
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap-icons/font/bootstrap-icons.css"></script>
</body>
</html>

